{
	"name": "Hackers and Hustlers",
	"version": "0.1",
	"manifest_version" : 2,
	"description": "Facebook extension for H&H",
	"content_scripts": [
	{
		"matches": ["https://www.facebook.com/groups/hackersandhustlers/*"],
		"js": ["lib/jquery-1.8.3.min.js","scripts/main.js"],
